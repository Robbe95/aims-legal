{
  "name": "payload-app",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "jobs:sync-single": "cross-env IS_JOB=TRUE pnpm payload jobs:run --queue sync --limit 1",
    "jobs:sync": "cross-env IS_JOB=TRUE pnpm payload jobs:run --queue sync --cron '*/2 * * * * *' --limit 30",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 5173",
    "dev:jobs": "IS_JOB=TRUE cross-env NODE_OPTIONS=--no-deprecation next dev -p 5174",
    "devturbo": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build": "pnpm build:cms && pnpm build:worker",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "node ./server.js",
    "start:worker": "node ./worker/worker.js",
    "start:jobs": "IS_JOB=TRUE node ./server.js",
    "lint:fix": "eslint --fix",
    "lint": "eslint",
    "ci": "pnpm build",
    "worker": "payload jobs:run --cron '*/3 * * * * *' --limit=2 --queue sync --handle-schedules",
    "preview:cms": "node --no-experimepntal-strip-types --env-file=.env.local ./.next/standalone/apps/payload/server.js",
    "preview:worker": "node --no-experimental-strip-types --env-file=.env.local ./.next/standalone/apps/payload/worker/worker.js",
    "generate:types": "payload generate:types",
    "generate:schema": "payload-graphql generate:schema",
    "test": "test",
    "clean": "rm -rf node_modules && rm -rf .output && rm -rf .next && rm -rf .dist",
    "standalone-script": "node ./src/scripts/standalone-script.js",
    "typecheck": "pnpm importmap && tsc --noEmit",
    "mc": "payload migrate:create && payload migrate && pnpm generate:types",
    "build:cms": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "build:worker": "tsdown --clean --dts",
    "importmap": "node ./scripts/addTypeAnnotationToImportMap.js"
  },
  "dependencies": {
    "@bynder/compact-view": "^5.0.3",
    "@faceless-ui/window-info": "catalog:backend",
    "@orpc/server": "catalog:orpc",
    "@payloadcms/db-postgres": "catalog:payload",
    "@payloadcms/next": "catalog:payload",
    "@payloadcms/plugin-multi-tenant": "catalog:payload",
    "@payloadcms/plugin-seo": "catalog:payload",
    "@payloadcms/richtext-lexical": "catalog:payload",
    "@payloadcms/storage-s3": "catalog:payload",
    "@payloadcms/storage-vercel-blob": "^3.64.0",
    "@payloadcms/ui": "catalog:payload",
    "@repo/constants": "workspace:*",
    "@repo/contract": "workspace:*",
    "@repo/models": "workspace:*",
    "@repo/payload-types": "workspace:*",
    "@repo/utils": "workspace:*",
    "body-scroll-lock": "catalog:backend",
    "cookies-next": "catalog:backend",
    "croner": "^9.1.0",
    "cross-env": "^7.0.3",
    "graphql": "catalog:backend",
    "jose": "catalog:backend",
    "next": "catalog:backend",
    "next-client-cookies": "catalog:backend",
    "payload": "catalog:payload",
    "pkce-challenge": "catalog:backend",
    "qs-esm": "^7.0.2",
    "react": "catalog:backend",
    "react-dom": "catalog:backend",
    "sharp": "catalog:backend",
    "unstorage": "catalog:backend",
    "zod": "catalog:shared"
  },
  "devDependencies": {
    "@types/body-scroll-lock": "catalog:types",
    "@types/node": "catalog:types",
    "@types/react": "catalog:types",
    "@types/react-dom": "catalog:types",
    "@wisemen/eslint-config-vue": "catalog:lint",
    "eslint": "catalog:lint",
    "eslint-config-next": "catalog:lint",
    "rolldown": "1.0.0-beta.38",
    "tsdown": "^0.14.2",
    "typescript": "catalog:build"
  }
}
