{
  "name": "payload-app",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 5173",
    "devturbo": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "node ./server.js",
    "lint:fix": "eslint --fix",
    "lint": "eslint",
    "ci": "pnpm build",
    "generate:types": "payload generate:types",
    "generate:schema": "payload-graphql generate:schema",
    "clean": "rm -rf node_modules && rm -rf .output && rm -rf .next && rm -rf .dist",
    "standalone-script": "node ./src/scripts/standalone-script.js",
    "typecheck": "pnpm importmap && tsc --noEmit",
    "mc": "payload migrate:create && payload migrate && pnpm generate:types",
    "importmap": "node ./scripts/addTypeAnnotationToImportMap.js"
  },
  "dependencies": {
    "@faceless-ui/window-info": "3.0.1",
    "@orpc/server": "catalog:orpc",
    "@payloadcms/db-postgres": "catalog:payload",
    "@payloadcms/next": "catalog:payload",
    "@payloadcms/plugin-multi-tenant": "catalog:payload",
    "@payloadcms/plugin-seo": "catalog:payload",
    "@payloadcms/richtext-lexical": "catalog:payload",
    "@payloadcms/storage-s3": "catalog:payload",
    "@payloadcms/ui": "catalog:payload",
    "@repo/constants": "workspace:*",
    "@repo/contract": "workspace:*",
    "@repo/models": "workspace:*",
    "@repo/payload-types": "workspace:*",
    "@repo/utils": "workspace:*",
    "body-scroll-lock": "4.0.0-beta.0",
    "cookies-next": "6.1.0",
    "cross-env": "10.0.0",
    "graphql": "16.11.0",
    "jose": "6.0.11",
    "next": "15.3.2",
    "next-client-cookies": "2.1.0",
    "payload": "catalog:payload",
    "pkce-challenge": "5.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.34.1",
    "zod": "catalog:types"
  },
  "devDependencies": {
    "@types/body-scroll-lock": "3.1.2",
    "@types/node": "22.15.17",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.4",
    "@wisemen/eslint-config-vue": "catalog:lint",
    "eslint": "catalog:lint",
    "eslint-config-next": "15.3.2",
    "typescript": "catalog:types"
  }
}
